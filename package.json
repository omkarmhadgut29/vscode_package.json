{
    "python.defaultInterpreterPath": "C:\\Python310\\python.exe",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    // "editor.suggest.localityBonus": true,
    "scm.autoReveal": false,
    "[php]": {
        "editor.formatOnSave": true
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    // "editor.suggestSelection": "first",
    "emmet.includeLanguages": {
        "django-html": "html",
        "jinja-html": "html",
        "html": "djanjo-html",
        "javascript": "javascriptreact"
    },
    "python.analysis.diagnosticSeverityOverrides": {
        "reportMissingImports": "none",
        "reportUndefinedVariable": "information"
    },
    "python.analysis.completeFunctionParens": true,
    "[django-html]": {
        "editor.quickSuggestions": {
            "other": true,
            "comments": true,
            "strings": true
        },
        "editor.suggest.showSnippets": true,
        "breadcrumbs.showMethods": true
    },
    "terminal.integrated.tabs.enabled": true,
    "python.diagnostics.sourceMapsEnabled": true,
    "python.linting.flake8CategorySeverity.E": "Hint",
    "python.linting.flake8CategorySeverity.F": "Warning",
    // "editor.snippetSuggestions": "top" /*make sure this is NOT "none"*/,
    // "editor.suggest.snippetsPreventQuickSuggestions": false,
    // "editor.quickSuggestions": {
    //   "other": true,
    //   "comments": true,
    //   "strings": true
    // }, //on 7.2.2023
    "editor.acceptSuggestionOnEnter": "on",
    // "editor.quickSuggestionsDelay": 10, //on 7.2.2023
    // "editor.suggestOnTriggerCharacters": false,
    // "editor.tabCompletion": "on",
    // "editor.wordBasedSuggestions": false,
    // "editor.parameterHints.enabled": false,
    "python.experiments.optOutFrom": ["All"],
    "python.linting.banditEnabled": true,
    "python.linting.mypyEnabled": true,
    "python.terminal.activateEnvInCurrentTerminal": true,
    "python.terminal.executeInFileDir": true,
    "python.formatting.provider": "none",
    "python.analysis.diagnosticMode": "workspace",
    "editor.accessibilityPageSize": 14,
    "editor.fontSize": 15,
    "files.associations": {
        "*.js": "javascriptreact",
        "*.html": "html"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.find.addExtraSpaceOnTop": true,
        "editor.smartSelect.selectLeadingAndTrailingWhitespace": true
    },
    "auto-close-tag.insertSpaceBeforeSelfClosingTag": true,
    "[python]": {
        "editor.autoClosingBrackets": "always",
        "editor.wordBasedSuggestions": "off"
    },
    // "editor.suggest.showStatusBar": true,
    "problems.showCurrentInStatus": true,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    // "editor.inlineSuggest.enabled": true, //on 7-2-2023
    "better-comments.highlightPlainText": true,
    "better-comments.multilineComments": true,
    /**
     ** Text color: to change text color
     *? type ctrl + shift + p and
     *? type "inspect editor"
     *? copy textmate scope
     */
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "variable.other.readwrite.jsx",
                "settings": {
                    "foreground": "#ffa2b9"
                }
            },
            {
                "scope": "string.unquoted.tag-string.django",
                "settings": {
                    "foreground": "#5ffff7"
                }
            },
            {
                "scope": "meta.function-call.arguments.python",
                "settings": {
                    "foreground": "#5ffff7"
                }
            },
            {
                "scope": "variable.other.constant.object.jsx",
                "settings": {
                    "foreground": "#e3bf7a"
                }
            },
            {
                "scope": "variable.other.property.jsx",
                "settings": {
                    "foreground": "#e06c75"
                }
            }
        ]
    },
    "python.analysis.autoSearchPaths": false,
    "html.format.templating": true,
    "liveServer.settings.CustomBrowser": "chrome",
    "liveServer.settings.donotShowInfoMsg": true,
    "editor.accessibilitySupport": "off",
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "terminal.integrated.fontFamily": "Consolas, 'Courier New', monospace",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "auto-close-tag.activationOnLanguage": [
        "xml",
        "php",
        "blade",
        "ejs",
        "js",
        "jinja",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "plaintext",
        "markdown",
        "vue",
        "liquid",
        "erb",
        "lang-cfml",
        "cfml",
        "HTML (EEx)",
        "HTML (Eex)",
        "plist"
    ],
    "auto-close-tag.SublimeText3Mode": true,
    "auto-close-tag.fullMode": true,
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "javascript.suggest.completeFunctionCalls": true,
    "typescript.suggest.completeFunctionCalls": true,
    "eslint.alwaysShowStatus": true,
    "eslint.codeActionsOnSave.rules": ["*semi*"],
    "eslint.format.enable": true,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "path-intellisense.extensionOnImport": true,
    "path-intellisense.autoTriggerNextSuggestion": true,
    "editor.snippetSuggestions": "top",
    "editor.suggest.matchOnWordStartOnly": false,
    "editor.suggest.showDeprecated": false,
    "editor.suggest.showEnums": false,
    "editor.suggest.showEnumMembers": false,
    "editor.codeActionsOnSave": {},
    "javascript.format.insertSpaceAfterConstructor": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
    "editor.trimAutoWhitespace": false,
    "diffEditor.maxComputationTime": 0,
    "liveServer.settings.donotVerifyTags": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 4
    },
    "editor.wordWrap": "on",
    "prettier.tabWidth": 4,
    "editor.smartSelect.selectLeadingAndTrailingWhitespace": false,
    "editor.indentSize": 4,
    "oneDarkPro.italic": false,
    "redhat.telemetry.enabled": true,
    "tabnine.experimentalAutoImports": true,
    "css.lint.unknownAtRules": "ignore",
    "window.zoomLevel": 1,
    "editor.formatOnSave": true,
    "workbench.iconTheme": "material-icon-theme",
    "vsicons.associations.folderDefault.folder": null
}
